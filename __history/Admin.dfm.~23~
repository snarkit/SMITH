  object BasicQuery: TIB_Query
    DatabaseName = 'Platinum:IMSDB'
    FieldsDisplayWidth.Strings = (
      'N.PREFERRED=104')
    IB_Connection = IMSDM.FBConnection
    SQL.Strings = (
      'select * from JPOrgs o'
      'join JPNames n'
      'on o.orgcode = n.orgcode'
      'where (n.preferred<>0)'
      'and (o.preferredname is null) ')
    EditSQL.Strings = (
      'UPDATE JPORGS O SET'
      '   O.ORGCODE = :ORGCODE,'
      '   O.FAMILY = :FAMILY,'
      '   O.POLNO = :POLNO,'
      '   O.DATEMODIFIED = :DATEMODIFIED,'
      '   O.CARD = :CARD,'
      '   O.COMMENTS = :COMMENTS,'
      '   O.USEDBY = :USEDBY,'
      '   O.PREFERREDNAME = :PREFERREDNAME'
      'WHERE'
      '   ORGID = :OLD_ORGID')
    KeyLinksAutoDefine = False
    RequestLive = True
    Left = 151
    Top = 27
  end
  object SecondQuery: TIB_Query
    DatabaseName = 'Platinum:IMSDB'
    FieldsDisplayWidth.Strings = (
      'N.PREFERRED=104')
    IB_Connection = IMSDM.FBConnection
    SQL.Strings = (
      'select * from JPOrgs o'
      'join JPNames n'
      'on o.orgcode = n.orgcode'
      'where (n.preferred<>0)'
      'and (o.preferredname is null) ')
    EditSQL.Strings = (
      'UPDATE JPORGS O SET'
      '   O.ORGCODE = :ORGCODE,'
      '   O.FAMILY = :FAMILY,'
      '   O.POLNO = :POLNO,'
      '   O.DATEMODIFIED = :DATEMODIFIED,'
      '   O.CARD = :CARD,'
      '   O.COMMENTS = :COMMENTS,'
      '   O.USEDBY = :USEDBY,'
      '   O.PREFERREDNAME = :PREFERREDNAME'
      'WHERE'
      '   ORGID = :OLD_ORGID')
    RequestLive = True
    Left = 216
    Top = 26
  end
