  object NamesForOrgQuery: TIB_Query
    ColumnAttributes.Strings = (
      'NAME=BLANKISNULL'
      'LANGUAGE=BLANKISNULL')
    DatabaseName = 'Fungi'
    FieldsAlignment.Strings = (
      'LANGUAGE=CENTER'
      'LN=CENTER')
    FieldsCharCase.Strings = (
      'LANGUAGE=UPPER')
    FieldsDisplayLabel.Strings = (
      'COMMENTS=Comments')
    FieldsGridLabel.Strings = (
      'LN=No'
      'NAME=Name'
      'AUTHORITY=Authority'
      'LANGUAGE=Lang')
    FieldsDisplayWidth.Strings = (
      'LN=15'
      'LANGUAGE=25'
      'AUTHORITY=194'
      'COMMENTS=0'
      'NAME=200')
    FieldsEditMask.Strings = (
      'L'
      'LANGUAGE=L')
    FieldsReadOnly.Strings = (
      'NAMEID=TRUE')
    FieldsVisible.Strings = (
      'NAMEID=FALSE'
      'ORGCODE=FALSE')
    IB_Connection = OrganismConnection
    SQL.Strings = (
      'SELECT ORGCODE'
      '     , NAMEID'
      '     , NAME '
      '     , AUTHORITY'
      '     , LANGUAGE'
      '     , LN'
      '     , COMMENTS'
      'FROM ORGNAMES '
      'WHERE ORGNAMES.ORGCODE = :orgcode')
    AutoFetchAll = True
    DefaultValues.Strings = (
      'LN=0'
      'LANGUAGE=A')
    DeleteSQL.Strings = (
      'DELETE FROM ORGNAMES '
      'WHERE'
      '   NAMEID = :OLD_NAMEID')
    EditSQL.Strings = (
      'UPDATE ORGNAMES SET'
      '   ORGNAMES.ORGCODE = :ORGCODE,'
      '   ORGNAMES.NAME = :NAME,'
      '   ORGNAMES.AUTHORITY = :AUTHORITY,'
      '   ORGNAMES.LANGUAGE = :LANGUAGE,'
      '   ORGNAMES.LN = :LN,'
      '   ORGNAMES.COMMENTS = :COMMENTS'
      'WHERE'
      '   NAMEID = :OLD_NAMEID')
    InsertSQL.Strings = (
      'INSERT INTO ORGNAMES('
      '   ORGCODE,'
      '   NAME,'
      '   AUTHORITY,'
      '   LANGUAGE,'
      '   LN,'
      '   COMMENTS)'
      'VALUES ('
      '   :ORGCODE,'
      '   :NAME,'
      '   :AUTHORITY,'
      '   :LANGUAGE,'
      '   :LN,'
      '   :COMMENTS)')
    KeyLinks.Strings = (
      'ORGNAMES.NAMEID')
    OrderingItems.Strings = (
      'LANGUAGE=LANGUAGE;LANGUAGE DESC'
      'LN=LN;LN DESC')
    OrderingLinks.Strings = (
      'LANGUAGE=ITEM=1'
      'LN=ITEM=2')
    Left = 32
    Top = 208
    ParamValues = (
      'ORGCODE=')
  end
