  object OrgByCodeQuery: TIB_Query
    DatabaseName = 'Fungi'
    FieldsCharCase.Strings = (
      'ORGCODE=UPPER')
    FieldsEditMask.Strings = (
      'ORGCODE=>LLLLL<A')
    FieldsReadOnly.Strings = (
      'ORGID=TRUE')
    FieldsVisible.Strings = (
      'ORGID=FALSE'
      'ORGCODE=TRUE'
      'IMS=TRUE')
    IB_Connection = OrganismConnection
    SQL.Strings = (
      'SELECT '
      '       ORGID'
      '     , ORGCODE'
      '     , IMS'
      '     , COMMENTS'
      '     , CARD'
      'FROM ORGANISMS'
      'WHERE ORGCODE = :orgcode')
    DefaultValues.Strings = (
      'IMS=True')
    DeleteSQL.Strings = (
      'DELETE FROM ORGANISMS WHERE ORGID = :OLD_ORGID')
    EditSQL.Strings = (
      'UPDATE ORGANISMS SET'
      '   ORGANISMS.ORGCODE = :ORGCODE,'
      '   ORGANISMS.IMS = :IMS,'
      '   ORGANISMS.COMMENTS = :COMMENTS,'
      '   ORGANISMS.CARD = :CARD'
      'WHERE'
      '   ORGID = :OLD_ORGID')
    InsertSQL.Strings = (
      'INSERT INTO ORGANISMS('
      '   ORGCODE,'
      '   IMS,'
      '   COMMENTS,'
      '   CARD)'
      'VALUES ('
      '   :ORGCODE,'
      '   :IMS,'
      '   :COMMENTS,'
      '   :CARD)')
    KeyLinks.Strings = (
      'ORGANISMS.ORGID')
    Left = 32
    Top = 144
    ParamValues = (
      'ORGCODE=')
  end
  object OrgByCodeDS: TIB_DataSource
